{
  "name": "lazy-collections",
  "version": "0.10.0",
  "description": "Collection of fast and lazy operations",
  "main": "./dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobinMalfait/lazy-collections.git"
  },
  "author": {
    "name": "Robin Malfait",
    "email": "malfait.robin@gmail.com"
  },
  "engines": {
    "node": ">=13"
  },
  "license": "MIT",
  "homepage": "https://github.com/RobinMalfait/lazy-collections",
  "files": [
    "dist"
  ],
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transform": {
      "\\.ts$": "@swc/jest"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "build": "tsup ./src/index.ts --format esm --clean --minify",
    "test": "jest",
    "tdd": "jest --watchAll",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@swc/core": "^1.3.52",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "fast-check": "^3.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "tslib": "^2.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
